{
  "name": "js-given",
  "version": "0.0.4",
  "description": "JavaScript frontend to jgiven",
  "main": "index.js",
  "scripts": {
    "postinstall": "node_modules/protractor/bin/webdriver-manager update",
    "lint": "check-node-version -q --node '< 4.0' || (echo \"Don't worry, ESLint takes some time ...\" && eslint . ./)",
    "lintFix": "check-node-version -q --node '< 4.0' || (echo \"Don't worry, ESLint takes some time ...\" && eslint --fix . ./)",
    "pretest": "npm run flow && npm run lint",
    "test": "npm run test_mocha && npm run test_jest && npm run test_jasmine && npm run test_ava && npm run test_protractor",
    "test_mocha": "mocha --opts spec/support/mocha-default.opts spec",
    "test_jest": "check-node-version -q --node '< 4.0' || jest --config .jest.conf.json --colors",
    "test_jest_watch": "npm run test_jest -- --watch",
    "test_jasmine": "node spec/support/jasmine-runner.js",
    "test_ava": "ava spec/**.spec.js -v",
    "test_protractor": "check-node-version -q --node '< 4.0' || protractor .protractor.conf.js",
    "flow": "node_modules/flow-bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsGiven/jsGiven.git"
  },
  "keywords": [
    "testing",
    "bdd",
    "jgiven",
    "tests",
    "given",
    "when",
    "then",
    "test"
  ],
  "author": "Florent Le Gall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsGiven/jsGiven/issues"
  },
  "homepage": "https://github.com/jsGiven/jsGiven#readme",
  "devDependencies": {
    "ava": "0.16.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "check-node-version": "1.1.2",
    "eslint": "3.4.0",
    "eslint-config-standard": "6.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.14.2",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "flow-bin": "0.32.0",
    "jasmine": "2.5.0",
    "jasmine-spec-reporter": "2.7.0",
    "jest-cli": "15.1.1",
    "mocha": "3.0.2",
    "protractor": "4.0.4",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "lodash": "4.15.0",
    "string-humanize": "1.0.0"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./spec/support/setup-tests.js"
    ],
    "babel": "inherit"
  }
}
